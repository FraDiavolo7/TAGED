WITH TNbVictoire AS (
    SELECT CASE WHEN (TC.elo1 <= 1100) THEN 'NOOB' ELSE 'PGM' END AS Elo,
           TC.Liste1 AS Liste1,
           TC.Liste2 AS Liste2,
           COUNT(*) AS NbVictoire
    FROM vw_combat_12 TC
    WHERE TC.gagnant IN (0, 1)
    GROUP BY Elo, Liste1, Liste2
        ), 
TNbCombat AS (
    SELECT CASE WHEN (TC.elo1 <= 1100) THEN 'NOOB' ELSE 'PGM' END AS Elo,
           TC.Liste1 AS Liste1,
           TC.Liste2 AS Liste2,
           COUNT(*) AS NbCombat
    FROM vw_combat_12 TC
    GROUP BY Elo, Liste1, Liste2
        ), 
TMain AS (
    SELECT TNV.Elo AS ELoRank,
           TNV.Liste1 AS Seq1,
           TNV.Liste2 AS Seq2,
           NbVictoire AS NbV,
           NbCombat AS NbC,
           CASE WHEN (TNV.Elo = 'PGM') THEN 0 ELSE CAST(ROUND(CAST(NbVictoire AS DECIMAL) / CAST(NbCombat AS DECIMAL), 2) * 100 AS INTEGER) END AS TxVictoire1,
           CASE WHEN (TNV.Elo = 'NOOB') THEN 0 ELSE CAST(ROUND(CAST(NbVictoire AS DECIMAL) / CAST(NbCombat AS DECIMAL), 2) * 100 AS INTEGER) END AS TxVictoire2
    FROM TNbVictoire TNV, TNbCombat TNC
    WHERE TNV.Elo = TNC.Elo
    AND TNV.Liste1 = TNC.Liste1
    AND TNV.Liste2 = TNC.Liste2
    GROUP BY ELoRank, Seq1, Seq2, NbV, NbC, TxVictoire1, TxVictoire2
        )
SELECT ELoRank, Seq1, Seq2, NbV, NbC, TxVictoire1, TxVictoire2
FROM TMain
WHERE NbC > 3
ORDER BY EloRank DESC, Seq1, Seq2;